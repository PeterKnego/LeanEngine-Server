<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>lean-engine-demo</application>
    <version>1</version>

    <!--Sessions MUST be enabled-->
    <sessions-enabled>true</sessions-enabled>

    <threadsafe>true</threadsafe>

    <admin-console>
        <page name="LeanEngine Settings" url="/admin/settings.jsp"/>
        <page name="OAuth Settings" url="/admin/oauth-settings.jsp"/>
        <page name="LeanEngine Scripts" url="/admin/scripts.jsp"/>
    </admin-console>

</appengine-web-app>
